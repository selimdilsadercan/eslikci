{
  "name": "eslikci",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "increment-version": "node scripts/increment-version.ts",
    "mobile": "npm run increment-version && next build --turbopack && npx cap sync && npm run apk",
    "apk": "cd android && ./gradlew assembleDebug && ./gradlew bundleRelease && cd .. && npm run open-aab",
    "open-apk": "xdg-open /home/meral/Desktop/eslikci/android/app/build/outputs/apk/debug",
    "aab": "npm run increment-version && cd android && ./gradlew bundleRelease && cd ..",
    "open-aab": "xdg-open /home/meral/Desktop/eslikci/android/app/build/outputs/bundle/release",
    "clean": "cd android && ./gradlew clean"
  },
